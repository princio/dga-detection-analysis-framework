{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/pslregex/pslregex/c",
                "/usr/include/postgresql/",
                "/usr/include/arpa",
                "/usr/include/"
            ],
            "defines": ["__USE_MISC"],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c99",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-libpq"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/windowing/src/**",
                    "${workspaceFolder}/dns_parser/src/**"
                ]
            }
        }
    ],
    "version": 4
}